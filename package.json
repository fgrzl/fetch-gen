{
  "name": "@fgrzl/fetch-gen",
  "version": "1.3.0",
  "description": "Generate @fgrzl/fetch clients from OpenAPI spec",
  "author": "smiggleworth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fgrzl/fetch-gen.git"
  },
  "homepage": "https://github.com/fgrzl/fetch-gen#readme",
  "keywords": [
    "fetch",
    "generator",
    "openapi",
    "codegen",
    "go"
  ],
  "type": "module",
  "bin": {
    "fetch-gen": "dist/shim.cjs"
  },
  "exports": {
    "import": "./dist/shim.mjs",
    "require": "./dist/shim.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist node_modules",
    "test": "vitest run",
    "fmt": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
