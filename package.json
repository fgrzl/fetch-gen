{
  "name": "@fgrzl/fetch-gen",
  "version": "0.0.0",
  "description": "Generate @fgrzl/fetch clients from OpenAPI spec",
  "author": "smiggleworth",
  "license": "MIT",
  "keywords": [
    "fetch",
    "generator",
    "openapi",
    "codegen",
    "go"
  ],
  "bin": {
    "fetch-gen": "dist/shim.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild src/shim.ts --bundle --platform=node --format=cjs --outfile=dist/shim.js",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^24.0.1",
    "esbuild": "^0.25.5",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
